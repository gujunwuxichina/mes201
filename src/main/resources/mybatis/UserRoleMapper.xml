<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gujun.mes201.dao.UserRoleMapper">

    <select id="listById" resultType="userRole" parameterType="int">
        select
            uId,rId
        from
            user_role_tab
        where
            uId=#{uId}
    </select>

</mapper>